programa {
  funcao inicio() {
    inteiro senha
    // pedir a senha
    escreva("Digite a senha: ")
    // ler a senha
    leia(senha)
    // se senha correta
      // informar "Acesso permitido"
    // senao estiver
      // acesso negado
    se(senha == 1234){
      escreva("S2 Acesso permitido <3")
    }senao{
      escreva("!! Acesso NEGADO !!")
    }
  }
}
